@model List<BlogStore.DataAccessLayer.DTOS.CategoryWithArticleCountDto>

<div class="sidebar-box">
    <h3 class="heading">Categories</h3>
    
    <ul class="categories">
        @foreach (var item in Model)
        {
            <li><a href="#">@item.CategoryName <span>@item.ArticleCount</span></a></li>

        }
       
    </ul>
</div>